{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "incremental": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/components/Maps/index.jsx",
        "src/components/Maps/mapStyles.js",
        "src/pages/clinicas.jsx",
        "src/pages/clinicas.tss",
        "src/pages/index.tsx",
        "src/components/BoxCarroussel/index.tsx",
        "src/components/BoxCarroussel/style.js", "src/pages/clinicas/[office].tsx",
    ],
    "exclude": [
        "node_modules"
    ]
}
